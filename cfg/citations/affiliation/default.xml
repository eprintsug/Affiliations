<?xml version="1.0" ?>
<!-- Affilation citation style (default) -->

<cite:citation xmlns="http://www.w3.org/1999/xhtml" xmlns:cite="http://eprints.org/ep3/citation" xmlns:epc="http://eprints.org/ep3/control">

<!-- affiliation citations (default) -->
<span class="ep_affiliation_citation">
  <epc:print expr='affiliationid'/>: 
  <epc:choose>
    <epc:when test="primary_afid"><epc:print expr='primary_afid'/></epc:when>
    <epc:otherwise>UNSPECIFIED</epc:otherwise>
  </epc:choose>
  <epc:choose>
    <epc:when test="name"><epc:print expr='name'/></epc:when>
    <epc:otherwise>UNSPECIFIED</epc:otherwise>
  </epc:choose>
</span>
</cite:citation>
